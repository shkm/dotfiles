defaults
auth on
tls on
protocol smtp
tls_starttls on

account personal
host $MAIL_PERSONAL_SMTP_HOST
port $MAIL_PERSONAL_SMTP_PORT
user $MAIL_PERSONAL_USERNAME
from $MAIL_PERSONAL_USERNAME
password $MAIL_PERSONAL_PASSWORD
logfile $HOME/.local/share/msmtp/log.log

account work
host $MAIL_WORK_SMTP_HOST
port $MAIL_WORK_SMTP_PORT
user $MAIL_WORK_USERNAME
from $MAIL_WORK_USERNAME
password $MAIL_WORK_PASSWORD
