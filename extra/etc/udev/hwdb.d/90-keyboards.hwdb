# IdeaPad 5 built-in keyboard
evdev:input:b0011v0001*
# caps to esc
  KEYBOARD_KEY_3a=esc

# CMStorm Rapid I
evdev:input:b0003v2516p0020e0111*
# caps to esc
  KEYBOARD_KEY_70039=esc

# Anne  Pro
# Do this because I don't want to reinstall the Anne app to modify
evdev:input:b0003v0483p5710*
# super to left alt
  KEYBOARD_KEY_700e2=leftmeta
# left alt to super
  KEYBOARD_KEY_700e3=leftalt
